	associado.sfa_ass_margem,
	associado.sfa_ass_nome
	FROM sfa_fec_fechamento
	INNER JOIN sfa_ass_associado  AS associado ON 
	 sfa_fec_fechamento.sfa_ass_id = associado.sfa_ass_id]]>
